        <!-- Company Stats Custom CSS -->
        <style>
            .company-stats {
                background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%);
                padding: 80px 0;
                margin: 40px 0;
            }
            
            .stats-item {
                padding: 30px 20px;
                background: white;
                border-radius: 15px;
                box-shadow: 0 10px 30px rgba(174, 28, 154, 0.1);
                transition: all 0.3s ease;
                height: 100%;
                border: 1px solid rgba(174, 28, 154, 0.1);
            }
            
            .stats-item:hover {
                transform: translateY(-10px);
                box-shadow: 0 20px 40px rgba(174, 28, 154, 0.15);
                border-color: #AE1C9A;
            }
            
            .stats-number {
                background: linear-gradient(45deg, #AE1C9A, #d63384);
                background-clip: text;
                -webkit-background-clip: text;
                -webkit-text-fill-color: transparent;
                font-family: 'Arial', sans-serif;
                animation: countUp 2s ease-out;
            }
            
            .stats-number .unit {
                font-size: 1.5rem;
                margin-left: 5px;
            }
            
            .stats-label {
                font-weight: 600;
                text-transform: uppercase;
                letter-spacing: 1px;
            }
            
            .stats-icon {
                transition: transform 0.3s ease;
            }
            
            .stats-item:hover .stats-icon {
                transform: scale(1.1) rotate(10deg);
            }
            
            .default-icon {
                transition: all 0.3s ease;
            }
            
            .stats-item:hover .default-icon {
                background: linear-gradient(45deg, #AE1C9A, #d63384) !important;
                transform: scale(1.1);
            }
            
            @keyframes countUp {
                from { opacity: 0; transform: translateY(20px); }
                to { opacity: 1; transform: translateY(0); }
            }
            
            /* Responsive */
            @media (max-width: 768px) {
                .company-stats {
                    padding: 60px 0;
                }
                .stats-item {
                    margin-bottom: 30px;
                }
                .stats-number {
                    font-size: 2rem !important;
                }
            }
        </style>
        
        <!-- Counter Animation Script -->
        <script>
            document.addEventListener('DOMContentLoaded', function() {
                const counters = document.querySelectorAll('.counter');
                const speed = 200; // Animation speed
                
                const animateCounter = (counter) => {
                    const originalValue = counter.textContent.trim();
                    const numericValue = originalValue.replace(/[^0-9]/g, '');
                    const nonNumericPart = originalValue.replace(/[0-9,]/g, '');
                    
                    if (numericValue) {
                        const target = parseInt(numericValue);
                        const increment = target / speed;
                        let current = 0;
                        
                        const timer = setInterval(() => {
                            current += increment;
                            if (current >= target) {
                                counter.textContent = target.toLocaleString() + nonNumericPart;
                                clearInterval(timer);
                            } else {
                                counter.textContent = Math.ceil(current).toLocaleString() + nonNumericPart;
                            }
                        }, 10);
                    }
                };
                
                // Intersection Observer for animation trigger
                const observer = new IntersectionObserver((entries) => {
                    entries.forEach(entry => {
                        if (entry.isIntersecting) {
                            const counter = entry.target.querySelector('.counter');
                            if (counter && !counter.classList.contains('animated')) {
                                counter.classList.add('animated');
                                animateCounter(counter);
                            }
                        }
                    });
                });
                
                document.querySelectorAll('.stats-item').forEach(item => {
                    observer.observe(item);
                });
            });
        </script>

