        <!--------------- company-stats-section---------------->
        <c:if test="${not empty companyStats}">
            <section class="company-stats">
                <div class="container">
                    <div class="stats-section">
                        <div class="row">
                            <c:forEach var="stat" items="${companyStats}" varStatus="status">
                                <div class="col-lg-3 col-md-6 col-sm-6 mb-4">
                                    <div class="stats-item text-center" data-aos="fade-up" data-aos-delay="${status.index * 100}">
                                        <div class="stats-icon mb-3">
                                            <c:choose>
                                                <c:when test="${stat.statIcon != null and not empty stat.statIcon}">
                                                    <i class="${stat.statIcon} fa-3x" style="color: #AE1C9A;"></i>
                                                </c:when>
                                                <c:otherwise>
                                                    <div class="default-icon" style="width: 60px; height: 60px; background: #AE1C9A; border-radius: 50%; margin: 0 auto; display: flex; align-items: center; justify-content: center;">
                                                        <i class="fas fa-chart-line fa-2x text-white"></i>
                                                    </div>
                                                </c:otherwise>
                                            </c:choose>
                                        </div>
                                        <div class="stats-content">
                                            <h3 class="stats-number" style="color: #AE1C9A; font-size: 2.5rem; font-weight: bold; margin-bottom: 10px;">
                                                <span class="counter">${stat.statValue}</span>
                                            </h3>
                                            <p class="stats-label" style="color: #666; font-size: 1.1rem; margin: 0;">
                                                ${stat.statName}
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </c:forEach>
                        </div>
                    </div>
                </div>
            </section>
        </c:if>
        <!--------------- company-stats-end---------------->

